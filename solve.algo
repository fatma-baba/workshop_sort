ALGORITHM insertion_sort
VAR
    i,j,key: INTEGER;
    a : ARRAY_OF INTEGER[100];
BEGIN
    FOR i FROM 1 TO a.length-1 STEP 1  DO
        key := a[i];
        j := i-1;
        WHILE (j>=0 AND a[j] > key) DO
            a[j+1] := a[j];
            j := j - 1;
        END_WHILE
        a[j+1] := key
    END_FOR 
END